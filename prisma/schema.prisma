generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Champions {
  id          String  @id
  key         BigInt?
  name        String?
  title       String?
  tags        String?
  stats       Float?
  description String?
  icon        String?
}
